{
    "version": 2,
    "builds": [
        {
            "src": "api/simple_test.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/simple_test.py"
        }
    ],
    "env": {
        "VERCEL": "1"
    },
    "functions": {
        "api/simple_test.py": {
            "maxDuration": 30
        }
    }
}