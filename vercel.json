{
    "version": 2,
    "builds": [
        {
            "src": "api/test.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/test.py"
        }
    ],
    "env": {
        "VERCEL": "1"
    },
    "functions": {
        "api/test.py": {
            "maxDuration": 30
        }
    }
}