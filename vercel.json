{
    "version": 2,
    "builds": [
        {
            "src": "api/working_api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/working_api.py"
        }
    ],
    "env": {
        "VERCEL": "1"
    },
    "functions": {
        "api/working_api.py": {
            "maxDuration": 30
        }
    }
}